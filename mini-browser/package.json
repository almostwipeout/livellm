{
  "name": "quad-browser",
  "version": "1.2.1",
  "description": "4分割AIブラウザ - ChatGPT, Gemini, Claude, Grok を同時操作 + MCP対応（赤兎馬ラウザー）",
  "main": "main.js",
  "bin": {
    "quad-mcp": "./mcp-server.js"
  },
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "mcp": "node mcp-server.js"
  },
  "keywords": [
    "electron",
    "browser",
    "ai",
    "chatgpt",
    "gemini",
    "claude",
    "grok",
    "mcp",
    "model-context-protocol"
  ],
  "author": "almostwipeout",
  "license": "MIT",
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1"
  },
  "build": {
    "appId": "com.quadbrowser.app",
    "productName": "Quad Browser",
    "win": {
      "target": "portable",
      "icon": "favicon.ico"
    },
    "directories": {
      "output": "dist"
    }
  }
}
